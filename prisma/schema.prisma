generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id                 String    @id @default(cuid())
  first_name         String
  last_name          String
  email              String    @unique
  password           String
  subscribe          Boolean
  createdAt          DateTime  @default(now())
  updatedAt          DateTime  @updatedAt
}

